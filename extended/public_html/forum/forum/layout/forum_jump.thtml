{# begin {templatelocation} #}

<form class="uk-form" action="{action}" method="post" name="jumpbox" onsubmit="if(document.jumpbox.forum.value == -1){return false;}">
	<div>
		<select name="forum" style="width:170px" onchange="this.form.submit();">
			<option value="-1">{LANG_msg106}</option>
			{selecthtml}
		</select>
	</div>
</form>

{# end {templatelocation} #}

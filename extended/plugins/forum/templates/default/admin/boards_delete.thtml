{# begin {templatelocation} #}

<form action="{PHP_SELF}" method="post">
    <table width="100%" border="0" cellspacing="0" cellpadding="3" class="plugin" style="margin-top:10px;">
        <tr>
            <td class="pluginAlert pluginMediumText" style="padding:10px;">{alert_title}</td>
        </tr>
        <tr>
            <td class="pluginCellText pluginCellFill" style="text-align:center;padding:10px;">
                <input type="hidden" name="type" value="{type}"{xhtml}>
                <input type="hidden" name="confirm" value="1"{xhtml}>
                <input type="hidden" name="id" value="{id}"{xhtml}>
                <input type="hidden" name="{gltoken_name}" value="{gltoken}"{xhtml}>
                {alert_message}
            </td>
        </tr>
        <tr>
            <td class="pluginCellText pluginCellFill" style="text-align:center;padding:10px;">
                <input type="submit" name="mode" value="{LANG_DELETE}"{xhtml}>&nbsp;<input type="button" value="{LANG_CANCEL}" onclick="javascript:history.go(-1)"{xhtml}>
            </td>
        </tr>
    </table>
</form>

{# end {templatelocation} #}
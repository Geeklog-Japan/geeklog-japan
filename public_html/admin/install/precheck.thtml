<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="{lang}" lang="{lang}">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta http-equiv="Content-Style-Type" content="text/css" />
	<meta http-equiv="Pragma" content="no-cache" />
	<link href="precheck.css" rel="stylesheet" type="text/css" />
	{javascript}
	<title>Precheck-{precheck_version} for Geeklog</title>
</head>
<body>
	<div class="header-navigation-container">
		<div class="header-navigation-line">
			<a href="http://www.geeklog.jp/forum/index.php?forum=6" class="header-navigation">{lang_help}</a>&nbsp;&nbsp;&nbsp;
		</div>
	</div>
	<div class="header-logobg-container-inner">
		<a class="header-logo" href="http://www.geeklog.net/"><img src="layout/logo.png"  width="151" height="56" alt="Geeklog" /></a>
		<div class="header-slogan">The Secure CMS.<br /><br /></div>
	</div>
	
	<div class="installation-container">
		<div class="installation-body-container">
			<h1>{lang_precheck}&nbsp;&nbsp;{lang_version}&nbsp;{precheck_version}</h1>
			
			<!-- Start of navigation bar -->
			<ul id="navi" class="navi" style="visibility: {nav_visibility};">
				<li class="{nav_step1_class}">{lang_step1}</li>
				<li class="{nav_step2_class}">{lang_step2}</li>
				<li class="{nav_step3_class}">{lang_step3}</li>
				<li class="{nav_step4_class}">{lang_step4}</li>
			</ul><br />
			<!-- End of navigation bar -->
			{content}
			<p class="precheck-version">{lang_precheck}&nbsp;&nbsp;{lang_version}&nbsp;{precheck_version}</p>
		</div>
	</div>
</body>
</html>
